{
  "name": "mariadb",
  "version": "0.0.1",
  "description": "fast mariadb/mysql driver.",
  "main": "index.js",
  "directories": {
    "src": "src",
    "test": "test"
  },
  "private": false,
  "scripts": {
    "test": "npm run test:lint && npm run test:prettier && npm run test:base",
    "test:base": "mocha \"test/**/*.js\"",
    "test:lint": "eslint index.js 'src/**/*.js' 'test/**/*.js'",
    "test:prettier": "prettier --print-width 100 --trailing-comma none --write \"{src,test,benchmarks}/**/*.js\"",
    "benchmark": "node ./benchmarks/benchmarks.js",
    "generate": "node ./test/tools/generate-mariadb.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mariadb/mariadb-connector-nodejs.git"
  },
  "keywords": [
    "mariadb",
    "mysql",
    "client",
    "server",
    "driver"
  ],
  "files": [
    "src",
    "index.js"
  ],
  "engines": {
    "node": ">= 4.0"
  },
  "author": "Diego Dupin <diego.dupin@mariadb.com>",
  "license": "LGPL-2.1+",
  "dependencies": {
    "denque": "^1.1.1",
    "iconv-lite": "^0.4.21",
    "long": "^4.0.0"
  },
  "devDependencies": {
    "benchmark": "^2.1.4",
    "chai": "^4.1.2",
    "colors": "^1.2.1",
    "error-stack-parser": "^2.0.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-async-await": "0.0.0",
    "eslint-plugin-markdown": "^1.0.0-beta.8",
    "mocha": "^5.1.1",
    "prettier": "^1.12.1",
    "prettier-markdown": "^0.1.6"
  },
  "bugs": {
    "url": "https://jira.mariadb.org/projects/CONJS/"
  },
  "homepage": "https://github.com/mariadb/mariadb-connector-nodejs#readme"
}
